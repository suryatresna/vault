<% content_for :title do %>
	Add Site
<% end %>

<% content_for :titleTag do %>
	The site of user permissions
<% end %>

<% content_for :body do %>

<div class="row">
	<div class="col-sm-12">
	  <div class="panel panel-white no-radius">
	    <div class="panel-body">
	      <h1>Site#new</h1>
	      <%= form_for @site, url: {action: "create"} do |f| %>
	      <%= render 'site/form', {f:f, permissions: @permissions} %>
	      <% end %>
	    </div>
	  </div>
	</div>
</div>
<% end %>

