<% content_for :title do %>
	Edit Permission
<% end %>

<% content_for :titleTag do %>
	The permission of user
<% end %>

<% content_for :body do %>

<div class="row">
	<div class="col-sm-12">
	  <div class="panel panel-white no-radius">
	    <div class="panel-body">
	      <h1>Permission#edit</h1>
		  <%= form_for @permission, url: {action: "update"}, method: 'put' do |f| %>
	      <%= render 'permission/form', {f:f} %>
	      <% end %>
	    </div>
	  </div>
	</div>
</div>
<% end %>

